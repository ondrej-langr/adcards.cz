            <div class="dropdown-language-around">
                <div class="dropdown language-btn">
                    <img src="/images/flags/{{ getCurrentLanguage() }}.svg" width="14" height="15"
                         alt="{{ getCurrentLanguage() }} vlajka">
                    {{ getCurrentLanguage() }}
                </div>
                <div class="dropdown-language-content">
                    {#                    We create special array that has keys as languages and value as empty string. This will be later used to clear url #}
                    {% set languagesAsReplacer = {} %}
                    {% set defaultLanguage = getConfig().i18n.default %}
                    {% for language in getConfig().i18n.languages %}
                        {% set languageWithSlash = language ~ "/" %}
                        {% set languagesAsReplacer = languagesAsReplacer|merge({
                            (languageWithSlash): ""
                        }) %}
                    {% endfor %}

                    {% for language in getConfig().i18n.languages %}
                        {% set finalUrl = current_url()|replace(languagesAsReplacer) %}
                        {% if language != defaultLanguage %}
                            {% set finalUrl = '/' ~ language ~ finalUrl %}
                        {% endif %}

                        <a href="{{ finalUrl }}">
                            <img alt='{{ language }} vlajka' src="/images/flags/{{ language }}.svg">
                            {{ (language) | upper }}
                        </a>
                    {% endfor %}
                </div>
            </div>

